{
  "extends": "@antfu",
  "ignorePatterns": [
    "*.md",
    "**/fixtures/**",
    "packages/metadata/metadata.ts"
  ],
  "rules": {
    "no-void": "off",
    "no-template-curly-in-string": "off",
    "no-mixed-operators": "off",
    "max-statements-per-line": "off",
    "no-cond-assign": "off"
  }
}
