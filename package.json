{
  "name": "@eslint-stylistic/monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r run dev",
    "lint": "eslint .",
    "release": "bumpp -r && pnpm -r publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.6.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.49.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.7.5",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
